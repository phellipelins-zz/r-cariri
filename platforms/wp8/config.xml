<?xml version='1.0' encoding='utf-8'?>
<widget id="br.phellipelins.rcariri" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="NetworkStatus">
        <param name="wp-package" value="NetworkStatus" />
    </feature>
    <feature name="SplashScreen">
        <param name="wp-package" value="SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <preference name="SplashScreen" value="www/res/drawable-port-ldpi/screen.png" />
    <splash density="port-ldpi" src="www/res/drawable-port-ldpi/screen.png" />
    <splash density="port-mdpi" src="www/res/drawable-port-mdpi/screen.png" />
    <splash density="port-hdpi" src="www/res/drawable-port-hdpi/screen.png" />
    <splash density="port-xhdpi" src="www/res/drawable-port-xhdpi/screen.png" />
    <icon density="ldpi" src="www/res/drawable-ldpi/icon.png" />
    <icon density="mdpi" src="www/res/drawable-mdpi/icon.png" />
    <icon density="hdpi" src="www/res/drawable-hdpi/icon.png" />
    <icon density="xhdpi" src="www/res/drawable-xhdpi/icon.png" />
    <preference name="Orientation" value="portrait" />
    <name>R Cariri</name>
    <description>
        Encontro Regional dos Estudantes de Design do Norte/Nordeste
    </description>
    <author email="phellipelins@gmail.com" href="http://phellipelins.github.io">
        Phellipe Lins
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
